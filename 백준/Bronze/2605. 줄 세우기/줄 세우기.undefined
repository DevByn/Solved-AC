#include <iostream>
#include <stack>
#include <vector>
#include <cmath>
#include <algorithm>

using namespace std;

int main() {

    int N;
    cin >> N;
    vector<int>v(N);

    for(int i = 0; i < N; i++){
        int a;
        cin >> a;

        for(int j = 0; j < a; j++){
            v[i-j] = v[i-1-j];
        }

        v[i-a] = i + 1;
    }
    
    for(auto & i : v){
        cout << i << ' ';
    }
}